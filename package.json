{
  "name": "nodejs_animations",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "worker": "node worker.js",
    "app:worker": "wait-on tcp:5005 && node --max-old-space-size=3000 worker.js",
    "clustered:worker": "wait-on tcp:5005 && node --max-old-space-size=3000 clusteredWorker.js",
    "start:dev": "concurrently -k -c \"blue,magenta\" \"npm:start\" \"npm:app:worker\"",
    "start:prod:clustered": "concurrently -k -c \"blue,magenta\" \"npm:start\" \"npm:clustered:worker\"",
    "eslint": "eslint ./",
    "eslint-fix": "eslint ./ --fix",
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.6.0",
    "async": "^3.2.0",
    "aws-config": "^1.4.0",
    "aws-sdk": "^2.799.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "cluster": "^0.7.7",
    "concurrently": "^5.3.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "global-mercator": "^3.1.0",
    "http": "0.0.1-security",
    "jimp": "^0.16.1",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "pify": "^5.0.0",
    "query-string": "^6.13.7",
    "stream": "0.0.2",
    "wait-on": "^5.2.0"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  }
}
